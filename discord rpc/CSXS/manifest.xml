<?xml version='1.0' encoding='UTF-8'?>
<!-- 1) -->
<ExtensionManifest ExtensionBundleId="com.discord.rpc.tee" ExtensionBundleVersion="0.0.3" Version="7.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ExtensionList>
    <!-- 2) -->
    <Extension Id="com.tee.discord.rpc" Version="0.0.3" />
    <Extension Id="com.tee.server" Version="0.0.1" />
  </ExtensionList>
  <ExecutionEnvironment>
    <HostList>
      <!-- 3) -->
      <Host Name="PHSP" Version="19" />
      <Host Name="PHXS" Version="19" />
      <Host Name="IDSN" Version="13" />
      <Host Name="AICY" Version="13" />
      <Host Name="ILST" Version="22" />
      <Host Name="PPRO" Version="12" />
      <Host Name="PRLD" Version="7" />
      <Host Name="AEFT" Version="15" />
      <Host Name="FLPR" Version="18" />
      <Host Name="AUDT" Version="11" />
      <Host Name="DRWV" Version="18" />
      <Host Name="MUSE" Version="2018" />
      <Host Name="KBRG" Version="8" />
      
    </HostList>
    <LocaleList>
      <Locale Code="All" />
    </LocaleList>
    <RequiredRuntimeList>
      <RequiredRuntime Name="CSXS" Version="7.0" />
    </RequiredRuntimeList>
  </ExecutionEnvironment>
  <DispatchInfoList>
    <!-- 2) -->
    <Extension Id="com.tee.discord.rpc">
      <DispatchInfo>
        <Resources>
          <!-- 4) -->
          <MainPath>./client/index.html</MainPath>
          <!-- 5) -->

          <ScriptPath>./host/core.jsx</ScriptPath>

          <CEFCommandLine />
        </Resources>
        <Lifecycle>
          <AutoVisible>false</AutoVisible>
          <StartOn>
            <Event>com.adobe.csxs.events.ApplicationInitialized</Event>
            <!-- <Event>com.adobe.csxs.events.AppOnline</Event> -->
            <Event>applicationActivate</Event>
          </StartOn>
        </Lifecycle>
        <UI>
          <Type>Panel</Type>
          <!-- 6) -->
          <Menu>Discord Rich Presence</Menu>
          <Geometry>
            <Size>
              <!-- 7) -->
              <Height>200</Height>
              <Width>350</Width>
            </Size>
          </Geometry>
          <Icons />
        </UI>
      </DispatchInfo>
    </Extension>
    <Extension Id="com.tee.server">
        <DispatchInfo>
      <Resources>
        <MainPath>./client/localServer.html</MainPath>
        <CEFCommandLine>
          <Parameter>--allow-file-access</Parameter>
					<Parameter>--allow-file-access-from-files</Parameter>
          <Parameter>--enable-nodejs</Parameter>
          <Parameter>--mixed-context</Parameter>
        </CEFCommandLine>
      </Resources>
      <Lifecycle>
        <AutoVisible>false</AutoVisible>
      </Lifecycle>
        <UI>
          <Type>Custom</Type>
          <Geometry>
            <Size>
              <Height>600</Height>
              <Width>600</Width>
            </Size>
          </Geometry>
        </UI>
    </DispatchInfo>
  </Extension>
  </DispatchInfoList>
</ExtensionManifest>